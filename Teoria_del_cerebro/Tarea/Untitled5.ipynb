   "source": [
    "# How to use collections.deque as a FIFO queue:\n",
    "\n",
    "from collections import deque\n",
    "q = deque()\n",
    "\n",
    "q.append('eat')\n",
    "q.append('sleep')\n",
    "q.append('code')\n",
    "\n",
    ">>> q\n",
    "deque(['eat', 'sleep', 'code'])\n",
    "\n",
    ">>> q.popleft()\n",
    "'eat'\n",
    ">>> q.popleft()\n",
    "'sleep'\n",
    ">>> q.popleft()\n",
    "'code'"
   ]
  }
 ],
